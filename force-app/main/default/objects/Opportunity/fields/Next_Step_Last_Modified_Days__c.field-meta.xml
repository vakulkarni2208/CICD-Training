<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Next_Step_Last_Modified_Days__c</fullName>
    <description>How many days since the next step notes was last modified.</description>
    <externalId>false</externalId>
    <formula>IF(IsClosed,

CloseDate - Next_Step_Last_Modified_Date__c ,

TODAY() - Next_Step_Last_Modified_Date__c

)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Next Step Last Modified (Days)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>0</scale>
    <trackHistory>false</trackHistory>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
