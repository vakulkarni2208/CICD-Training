    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND(
          BEGINS([Case].OwnerId ,&apos;005&apos;),
         [Case].RecordType.Name=&apos;Community User Record Type&apos;  ,
        OR(
                ISNEW() ,
                AND(
                          ISCHANGED([Case].OwnerId ),
                          [Case].OwnerId &lt;&gt;$User.Id 
