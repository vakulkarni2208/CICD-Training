TEXT({!myVariable_current.TimeZone__c}) &lt;&gt; TEXT({!myVariable_current.Community_User__r.TimeZoneSidKey}) 
),
TEXT({!myVariable_current.TimeZone__c}) &lt;&gt; null,
NOT(CONTAINS(TEXT({!$User.UserType}) , &apos;PowerCustomer&apos;)),
{!myVariable_current.Community_User__c}&lt;&gt;null , 
	{!myVariable_current.Community_User_Active__c}
)</expression>
    </formulas>
