<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Last_Activity_For_Auto_Close__c</fullName>
    <description>If LastActiveDate &gt; LastModifiedDate; LastActiveDate + 30 else LastModifiedDate + 30</description>
    <externalId>false</externalId>
    <formula>if( LastActivityDate &gt; DATEVALUE(LastModifiedDate), LastActivityDate+30  , DATEVALUE(LastModifiedDate)+30 )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Last Activity For Auto Close</label>
    <required>false</required>
    <trackHistory>false</trackHistory>
    <type>Date</type>
</CustomField>
