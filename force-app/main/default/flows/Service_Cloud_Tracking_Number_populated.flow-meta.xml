OR([RMA__c].RMA_Status__c=&apos;INPR&apos;,[RMA__c].RMA_Status__c=&apos;SHIP&apos;,[RMA__c].RMA_Status__c=&apos;COMP&apos;,[RMA__c].RMA_Status__c=&apos;OPEN&apos;)
