    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>
(ISPICKVAL([Case].Status ,&quot;Closed&quot;)) &amp;&amp; 
NOT(ISCHANGED([Case].Incident_Status__c ))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_3</name>
        <dataType>Boolean</dataType>
        <expression>(ISPICKVAL({!myVariable_current.Status} ,&quot;Closed&quot;)) &amp;&amp; 
NOT(ISCHANGED({!myVariable_current.Incident_Status__c} ))</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>(ISCHANGED([Case].Incident_Status__c ) &amp;&amp; 

(ISPICKVAL(PRIORVALUE([Case].Incident_Status__c ),&quot;Troubleshooting-Sub&quot;))&amp;&amp;

ISPICKVAL([Case].Status,&quot;New&quot;)&amp;&amp;

NOT(ISPICKVAL([Case].Incident_Status__c,&quot;Closed&quot;))) ||

ISPICKVAL([Case].Status,&quot;Awaiting Customer Response&quot;) &amp;&amp; 

(ISPICKVAL(PRIORVALUE([Case].Incident_Status__c), &quot;Awaiting Customer Information&quot; )||

ISPICKVAL(PRIORVALUE([Case].Incident_Status__c), &quot;Pending - No Customer Response&quot;) )


</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_5</name>
        <dataType>Boolean</dataType>
        <expression>(ISCHANGED({!myVariable_current.Incident_Status__c} ) &amp;&amp; 

(ISPICKVAL(PRIORVALUE({!myVariable_current.Incident_Status__c} ),&quot;Troubleshooting-Sub&quot;))&amp;&amp;

ISPICKVAL({!myVariable_current.Status},&quot;New&quot;)&amp;&amp;

NOT(ISPICKVAL({!myVariable_current.Incident_Status__c},&quot;Closed&quot;))) ||

ISPICKVAL({!myVariable_current.Status},&quot;Awaiting Customer Response&quot;) &amp;&amp; 

(ISPICKVAL(PRIORVALUE({!myVariable_current.Incident_Status__c}), &quot;Awaiting Customer Information&quot; )||

ISPICKVAL(PRIORVALUE({!myVariable_current.Incident_Status__c}), &quot;Pending - No Customer Response&quot;) )</expression>
    </formulas>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>ISPICKVAL([Case].Incident_Status__c, &quot;Closed&quot;) &amp;&amp; NOT(ISCHANGED([Case].Status))</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_7</name>
        <dataType>Boolean</dataType>
        <expression>ISPICKVAL({!myVariable_current.Incident_Status__c}, &quot;Closed&quot;) &amp;&amp; NOT(ISCHANGED({!myVariable_current.Status}))</expression>
    </formulas>
