    <rules>
        <fullName>Auto emails to DLI Lead Record Type</fullName>
        <actions>
            <name>Auto_response_to_a_DLI_Lead_submission</name>
            <type>Alert</type>
        </actions>
        <actions>
            <name>Email_alert_to_DLI_Lead_Team_Internal</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Lead.RecordTypeId</field>
            <operation>equals</operation>
            <value>DLI Lead</value>
        </criteriaItems>
        <description>Used for auto response to Leads as native feature doesn&apos;t allow use of the DLI email address</description>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Initial Lead Class</fullName>
        <actions>
            <name>Update_Initial_Lead_Class_field</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <description>To display the value of Lead Class first populated (~ &quot;A2&quot;) i.e. one time stamp when Account Strength gets A/B/C/D. As per Jira #SDs-2961</description>
        <formula>NOT( ISNULL( Account_Strength__c ) )</formula>
        <triggerType>onCreateOnly</triggerType>
    </rules>
    <rules>
        <fullName>Lead %3A VGPU Eval Pending Approval Notification V2</fullName>
        <actions>
            <name>VGPU_Eval_Pending_Approval_Notification</name>
            <type>Alert</type>
        </actions>
        <active>true</active>
        <description>Lead : VGPU Eval Pending Approval Notification</description>
        <formula>NOT( ISBLANK(GRID_Software_Evaluation_PAK__c)) &amp;&amp; ISCHANGED( GRID_Software_Evaluation_PAK__c ) &amp;&amp; ISPICKVAL( GRID_Software_Evaluation__c , &apos;Pending Re-approval&apos;)</formula>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Lead is created from Marketo set sync with Marketo</fullName>
        <actions>
            <name>Sync_with_Marketo</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>Lead.Email</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Sync_with_Marketo__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.CreatedById</field>
            <operation>equals</operation>
            <value>Marketo Sync</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.MKTO_GDPR_Delete__c</field>
            <operation>notEqual</operation>
            <value>True</value>
        </criteriaItems>
        <description>Special sync with leads with Marketo. Lead is created from Marketo.</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Lead is created set sync with Marketo</fullName>
        <actions>
            <name>Sync_with_Marketo</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <booleanFilter>1 AND 2 AND 3 AND 4</booleanFilter>
        <criteriaItems>
            <field>Lead.Email</field>
            <operation>notEqual</operation>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.Sync_with_Marketo__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.RecordTypeId</field>
            <operation>notEqual</operation>
            <value>CPQ Requests,Deal Registration,DLI Lead,NPN Web Onboard Form</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.MKTO_GDPR_Delete__c</field>
            <operation>notEqual</operation>
            <value>True</value>
        </criteriaItems>
        <description>Updated this WF for the SDS-3014</description>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
    </rules>
    <rules>
        <fullName>Set Lead Record Type to Enterprise Customer Lead</fullName>
        <actions>
            <name>Set_Lead_Record_Type_to_Enterprise_Custo</name>
