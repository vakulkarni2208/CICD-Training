<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Company</field>
        <field>Name</field>
        <field>Owner</field>
        <field>DR_Partner_Sales_Rep_Company__c</field>
        <field>DR_Partner_Sales_Rep_Name__c</field>
        <field>DR_Deal_Type__c</field>
        <field>DR_Program_Type__c</field>
        <field>DR_Product__c</field>
        <field>JetsonDR_Annual_Quantity__c</field>
        <field>DR_Program_Name__c</field>
        <field>JetsonDR_Production_Start_Date__c</field>
        <field>JetsonDR_Production_End_Date__c</field>
        <field>Primary_Customer_Application__c</field>
        <field>DR_POC_Customer_Use_Case__c</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>DR_Approved_Date_Channel_Admin_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>DR_Is_Approved_By_Channel_Admin_T</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>DR_ChannelAdmin__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <label>Channel Admin Approval</label>
        <name>Channel_Admin_Approval</name>
        <rejectionActions>
            <action>
                <name>DR_Denied_Date_Channel_Admin_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>DR_Is_Denied_by_Channel_Admin_T</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>DR_Approved_Date_Sales_Rep_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>DR_Is_Approved_By_Sales_Rep_T</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>JetsonDR_Owner__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <entryCriteria>
            <criteriaItems>
                <field>Lead.DR_Is_Approved_by_Channel_Admin__c</field>
                <operation>equals</operation>
                <value>True</value>
            </criteriaItems>
        </entryCriteria>
        <label>Sales Rep Approval</label>
        <name>Sales_Rep_Approval</name>
        <rejectBehavior>
            <type>RejectRequest</type>
        </rejectBehavior>
        <rejectionActions>
            <action>
                <name>DR_Denied_Date_Sales_Rep_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>DR_Is_Denied_by_Sales_Rep_T</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <description>Jetson Deal Registration : Approval by Channel Admin and Sales Rep</description>
    <emailTemplate>Deal_Registration/X1_Jetson_Registration_Deal_Submitted_Email_To_NVIDIA</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Lead.RecordType</field>
            <operation>equals</operation>
            <value>Jetson Deal Registration</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.DR_Is_Submitted__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>DR_Approved_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Is_Approved_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X2_Jetson_Registration_Deal_Approved_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>DR_Denied_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Is_Denied_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X2_Jetson_Registration_Deal_Rejected_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>DR_Is_Submitted_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_In_Review</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Submitted_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_SubmittedViaBtnClick_F</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>JetsonDR_UpdateOwnerAddSharing_True</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X1_Jetson_Registration_Deal_Submitted_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </initialSubmissionActions>
    <label>Jetson Deal Registration</label>
    <processOrder>2</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
