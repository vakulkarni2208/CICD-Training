         {!myVariable_current.RecordType.Name}=&apos;Community User Record Type&apos;  ,
        OR(
                ISNEW() ,
                AND(
                          ISCHANGED({!myVariable_current.OwnerId} ),
                          {!myVariable_current.OwnerId} &lt;&gt;{!$User.Id} 
                         )
