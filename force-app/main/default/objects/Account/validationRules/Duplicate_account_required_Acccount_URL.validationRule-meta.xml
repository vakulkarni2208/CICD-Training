<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Duplicate_account_required_Acccount_URL</fullName>
    <active>true</active>
    <description>Account URL required when a user selects &quot;Duplicate account&quot; for Reason for Rejection</description>
    <errorConditionFormula>AND(ISPICKVAL(Reason_for_Rejection__c,&quot;Duplicate Account&quot;), ISBLANK(Account_URL__c))</errorConditionFormula>
    <errorDisplayField>Account_URL__c</errorDisplayField>
    <errorMessage>Account URL Required</errorMessage>
</ValidationRule>
