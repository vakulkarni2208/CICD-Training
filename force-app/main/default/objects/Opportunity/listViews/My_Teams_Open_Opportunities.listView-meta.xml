<?xml version="1.0" encoding="UTF-8"?>
<ListView xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>My_Teams_Open_Opportunities</fullName>
    <columns>OPPORTUNITY.NAME</columns>
    <columns>ACCOUNT.NAME</columns>
    <columns>OPPORTUNITY.STAGE_NAME</columns>
    <columns>OPPORTUNITY.CLOSE_DATE</columns>
    <columns>OPPORTUNITY.AMOUNT</columns>
    <columns>CORE.USERS.ALIAS</columns>
    <filterScope>Team</filterScope>
    <filters>
        <field>OPPORTUNITY.PROBABILITY</field>
        <operation>lessThan</operation>
        <value>100</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.RECORDTYPE</field>
        <operation>equals</operation>
        <value>Opportunity.Enterprise_Design_Wins,Opportunity.Enterprise_Sales_Opportunity</value>
    </filters>
    <filters>
        <field>OPPORTUNITY.PROBABILITY</field>
        <operation>greaterThan</operation>
        <value>0</value>
    </filters>
    <filters>
        <field>Industry__c</field>
        <operation>equals</operation>
        <value>Media/Entertainment,Telecommunications</value>
    </filters>
    <filters>
        <field>CORE.USERS.FIRST_NAME</field>
        <operation>contains</operation>
        <value>Terry,DJ,Arjuna,Tim</value>
    </filters>
    <label>My Teams Open Opportunities</label>
    <sharedTo>
        <role>NALA_Telco_M_and_E_Sales</role>
        <role>NALA_Telco_M_and_E_Sales_Manager</role>
    </sharedTo>
</ListView>
