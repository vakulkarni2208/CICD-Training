    <description>SDS-2581 Service: View Customer TimeZone, Address on a User</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND ( ISCHANGED([Contact].LastModifiedDate )  , 
OR(
[Contact].Community_User__r.Street &lt;&gt; [Contact].MailingStreet ,
[Contact].Community_User__r.City &lt;&gt; [Contact].MailingCity ,
[Contact].Community_User__r.State &lt;&gt; [Contact].MailingState ,
[Contact].Community_User__r.PostalCode&lt;&gt; [Contact].MailingPostalCode ,
[Contact].Community_User__r.Country&lt;&gt; [Contact].MailingCountry,
TEXT([Contact].TimeZone__c) &lt;&gt; TEXT([Contact].Community_User__r.TimeZoneSidKey) 
),
TEXT([Contact].TimeZone__c) &lt;&gt; null,
NOT(CONTAINS(TEXT($User.UserType) , &apos;PowerCustomer&apos;)),
[Contact].Community_User__c&lt;&gt;null , 
	[Contact].Community_User_Active__c
)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND ( ISCHANGED({!myVariable_current.LastModifiedDate} )  , 
OR(
{!myVariable_current.Community_User__r.Street} &lt;&gt; {!myVariable_current.MailingStreet} ,
