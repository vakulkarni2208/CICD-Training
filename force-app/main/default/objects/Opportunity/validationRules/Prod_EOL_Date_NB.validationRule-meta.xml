<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Prod_EOL_Date_NB</fullName>
    <active>true</active>
    <errorConditionFormula>AND(
( Design_EOL_Date__c &lt; Design_Start_Date__c ),
($RecordType.Name = &quot;Notebook Opportunity&quot;))</errorConditionFormula>
    <errorDisplayField>Design_EOL_Date__c</errorDisplayField>
    <errorMessage>Production EOL Date must be AFTER Start Date.  Please correct.</errorMessage>
</ValidationRule>
