<?xml version="1.0" encoding="UTF-8"?>
<SharingRules xmlns="http://soap.sforce.com/2006/04/metadata">
    <sharingGuestRules>
        <fullName>Share_Customer_Engagement_Survey</fullName>
        <accessLevel>Read</accessLevel>
        <description>Customer Engagement Survey</description>
        <label>Share Customer Engagement Survey</label>
        <sharedTo>
            <guestUser>ESPCommunity</guestUser>
        </sharedTo>
        <criteriaItems>
            <field>Name</field>
            <operation>equals</operation>
            <value>Customer Engagement Survey</value>
        </criteriaItems>
