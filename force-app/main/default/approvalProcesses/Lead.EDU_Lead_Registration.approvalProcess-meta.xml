<?xml version="1.0" encoding="UTF-8"?>
<ApprovalProcess xmlns="http://soap.sforce.com/2006/04/metadata">
    <active>true</active>
    <allowRecall>false</allowRecall>
    <allowedSubmitters>
        <type>creator</type>
    </allowedSubmitters>
    <allowedSubmitters>
        <type>owner</type>
    </allowedSubmitters>
    <approvalPageFields>
        <field>Name</field>
        <field>Email</field>
        <field>Company</field>
        <field>Description</field>
        <field>Industry</field>
        <field>DR_Program_Name__c</field>
        <field>DR_Deal_Type__c</field>
        <field>DR_Product__c</field>
        <field>DR_SKU_Quantity__c</field>
        <field>Owner</field>
    </approvalPageFields>
    <approvalStep>
        <allowDelegate>true</allowDelegate>
        <approvalActions>
            <action>
                <name>NPNPD_Approved_Date_PBM_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>NPNPD_Is_Approved_By_PBM_T</name>
                <type>FieldUpdate</type>
            </action>
        </approvalActions>
        <assignedApprover>
            <approver>
                <name>DR_ChannelAdmin__c</name>
                <type>relatedUserField</type>
            </approver>
            <whenMultipleApprovers>FirstResponse</whenMultipleApprovers>
        </assignedApprover>
        <description>Channel Admin for the Lead is set from Process Builder based on the Lead Country</description>
        <label>Channel Admin</label>
        <name>Channel_Admin</name>
        <rejectionActions>
            <action>
                <name>NPNPD_Denied_Date_PBM_Now</name>
                <type>FieldUpdate</type>
            </action>
            <action>
                <name>NPNPD_Is_Denied_By_PBM_T</name>
                <type>FieldUpdate</type>
            </action>
        </rejectionActions>
    </approvalStep>
    <description>SDS-1773) Sales: Updating Routing - NPN Program Deal
NPN Program Deal : Approval  process for Channel Admin only
Record type changed from NPN Program Deal to EDU Lead Registration</description>
    <emailTemplate>NPN_Program_Deal/X1_NPN_Program_Deal_Deal_Submitted_Email_To_NVIDIA</emailTemplate>
    <enableMobileDeviceAccess>true</enableMobileDeviceAccess>
    <entryCriteria>
        <criteriaItems>
            <field>Lead.RecordType</field>
            <operation>equals</operation>
            <value>EDU Lead Registration</value>
        </criteriaItems>
        <criteriaItems>
            <field>Lead.DR_Is_Submitted__c</field>
            <operation>equals</operation>
            <value>False</value>
        </criteriaItems>
    </entryCriteria>
    <finalApprovalActions>
        <action>
            <name>DR_Approved_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Is_Approved_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_Approved</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X2_NPN_Program_Deal_Deal_Approved_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </finalApprovalActions>
    <finalApprovalRecordLock>false</finalApprovalRecordLock>
    <finalRejectionActions>
        <action>
            <name>DR_Denied_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Is_Denied_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_Rejected</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X2_NPN_Program_Deal_Deal_Rejected_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </finalRejectionActions>
    <finalRejectionRecordLock>false</finalRejectionRecordLock>
    <initialSubmissionActions>
        <action>
            <name>DR_Is_Submitted_T</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Lead_Status_In_Review</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>DR_Submitted_Date_Now</name>
            <type>FieldUpdate</type>
        </action>
        <action>
            <name>X1_NPN_Program_Deal_Deal_Submitted_Email_To_Partner</name>
            <type>Alert</type>
        </action>
    </initialSubmissionActions>
    <label>EDU Lead Registration</label>
    <processOrder>4</processOrder>
    <recordEditability>AdminOrCurrentApprover</recordEditability>
    <showApprovalHistory>true</showApprovalHistory>
</ApprovalProcess>
